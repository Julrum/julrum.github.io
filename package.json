{
  "homepage": "https://julrum.github.io/",
  "name": "react-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env ANALYZE=false next build",
    "start": "next start",
    "lint": "next lint",
    "export": "next export",
    "predeploy": "yarn build",
    "deploy": "rm -rf node_modules/.cache && next build && next export && touch out/.nojekyll && git add -f out/ && git checkout -b temp-for-deploy-gh-pages && git commit -m \"Deploy Next.js to gh-pages\" && git subtree split --prefix out -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages && git checkout master && git branch -D temp-for-deploy-gh-pages"
  },
  "dependencies": {
    "@mdx-js/react": "^2.1.3",
    "@next/bundle-analyzer": "^12.3.1",
    "@next/mdx": "^12.3.1",
    "@types/react-syntax-highlighter": "^15.5.5",
    "@types/styled-components": "^5.1.26",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "gray-matter": "^4.0.3",
    "next": "12.3.1",
    "next-mdx-remote": "^4.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-code-titles": "^1.1.0",
    "rehype-highlight": "^5.0.2",
    "rehype-slug": "^5.0.1",
    "remark-emoji": "^3.0.2",
    "remark-gfm": "^3.0.1",
    "remark-supersub": "^1.0.0",
    "styled-components": "^5.3.6",
    "styled-normalize": "^8.0.7",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "18.7.23",
    "@types/react": "18.0.21",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.2.1",
    "gh-pages": "^4.0.0",
    "prettier": "^2.7.1",
    "typescript": "4.8.3"
  }
}
